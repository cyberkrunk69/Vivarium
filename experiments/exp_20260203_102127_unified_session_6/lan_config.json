{
  "session_timeout": 3600,
  "max_concurrent_sessions": 10,
  "user_workspace_quota": "10GB",
  "resource_limits": {
    "cpu_cores": 2,
    "memory_mb": 4096,
    "execution_timeout": 300
  },
  "admin_ips": ["192.168.1.1", "127.0.0.1"],
  "allowed_subnets": ["192.168.1.0/24", "127.0.0.0/8"],
  "base_workspace_dir": "user_sessions",
  "websocket_port": 8765,
  "web_port": 8080,
  "enable_audit_log": true,
  "log_level": "INFO"
}